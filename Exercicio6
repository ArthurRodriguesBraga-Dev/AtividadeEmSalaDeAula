int ano;
Console.WriteLine("Neste programa iremos saber se o ano que você digitar é bissexto ou não");
Console.Write("Digite o ano (Com 4 DIGITOS): ");
ano = Convert.ToInt32(Console.ReadLine());
if (ano%400 == 0 || ano%4 == 0 && ano%100 != 0)
{
    Console.WriteLine($"O ano {ano} é Bissexto");
}
else
{
    Console.WriteLine($"O ano {ano} não é Bissexto");
}
Console.ReadKey();
